{
  "name": "deric-journald",
  "version": "0.1.0",
  "author": "deric",
  "summary": "Systemd journal management",
  "license": "Apache-2.0",
  "source": "https://github.com/deric/puppet-journald",
  "project_page": "https://github.com/deric/puppet-journald",
  "issues_url": "https://github.com/deric/puppet-journald/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 2.3.3 < 5.0.0"
